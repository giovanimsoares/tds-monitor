{
  "name": "tds-monitor",
  "displayName": "TDS Monitor for VSCode",
  "description": "%tds.package.description%",
  "version": "0.1.0",
  "publisher": "TOTVS",
  "author": {
    "name": "TOTVS",
    "email": "tecnologia@totvs.com.br"
  },
  "license": "MIT",
  "keywords": [
    "advpl",
    "tlpp",
    "4gl",
    "tds",
    "totvs",
    "monitor"
  ],
  "engines": {
    "vscode": "^1.29.0",
    "node": "^8.9.3"
  },
  "icon": "imagens/tds-monitor.png",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/totvs/tds-monitor.git"
  },
  "bugs": {
    "url": "https://github.com/totvs/tds-monitor/issues"
  },
  "scripts": {
    "prepare": "tsc -p ./src",
    "clear": "del ./out",
    "compile": "npm-run-all compile:*",
    "watch": "npm-run-all -p watch:*",
    "compile:extension": "tsc -p ./src",
    "compile:views": "webpack --mode development",
    "watch:extension": "tsc -w -p ./src",
    "watch:views": "webpack --watch --mode development",
    "tslint": "tslint ./src/**/*.ts",
    "package": "npm run compile && vsce package",
    "publish": "vsce publish"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.50",
    "@totvs/tds-da": "^1.1.3",
    "@totvs/tds-ls": "^1.1.5",
    "@types/jszip": "^3.1.7",
    "@types/node": "^13.13.0",
    "@types/react": "^16.9.34",
    "@types/vscode": "^1.44.0",
    "core-js": "^3.6.5",
    "ini": "^1.3.5",
    "jszip": "^3.4.0",
    "material-table": "^1.57.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "strip-json-comments": "^3.1.0",
    "template-literal": "^1.0.3",
    "tmp": "^0.1.0",
    "typeface-roboto": "0.0.75",
    "typescript": "3.8.3",
    "vscode-debugadapter": "^1.40.0",
    "vscode-languageclient": "^6.1.3",
    "vscode-nls": "^4.1.2",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/ini": "^1.3.30",
    "@types/react-dom": "^16.9.6",
    "cleanup": "^0.3.0",
    "css-loader": "^3.5.2",
    "npm-run-all": "^4.1.5",
    "style-loader": "^1.1.4",
    "ts-loader": "^7.0.1",
    "tslint": "6.1.1",
    "vsce": "^1.75.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.10"
  },
  "main": "./out/extension",
  "activationEvents": [
    "onView:tds_monitor",
    "onCommand:tds-monitor.*"
  ],
  "contributes": {
    "languages": [],
    "grammars": [],
    "snippets": [],
    "configurationDefaults": {},
    "configuration": {
      "type": "object",
      "title": "TDS Monitor",
      "properties": {
        "tdsMonitor.servers.json": {
          "scope": "window",
          "type": "string",
          "enum": [
            "user",
            "user_monitor"
          ],
          "default": "user",
          "description": "Indica qual arquivo de definição de servidores a utilizar.",
          "enumDescriptions": [
            "Utiliza o arquivo [<user_home>/.totvslsv/server.json], ou seja, o mesmo do TDS-VSCode.",
            "Utiliza o arquivo [<user_home>/.totvslsv/monitor.json]."
          ]
        },
        "tdsMonitor.welcomePage": {
          "scope": "window",
          "type": "boolean",
          "default": true,
          "description": "Apresentar a página de boas vindas do TDS Monitor ao iniciar."
        },
        "tdsMonitor.trace.server": {
					"scope": "window",
					"type": "string",
					"enum": [
						"off",
						"messages",
						"verbose"
					],
					"default": "off",
					"description": "Traces the communication between VS Code and the language server."
				}
      }
    },
    "commands": [
      {
        "command": "tds-monitor.open.configuration",
        "title": "Configuração",
        "category": "TDSMonitor",
        "icon": {
          "light": "resources/light/settings.svn",
          "dark": "resources/dark/settings.svn"
        }
      },
      {
        "command": "tds-monitor.showWelcomePage",
        "title": "Boas vindas",
        "category": "TDSMonitor"
      },
      {
        "command": "tds-monitor.create",
        "title": "Novo servidor",
        "icon": {
          "light": "resources/light/add.png",
          "dark": "resources/dark/add.png"
        },
        "category": "TDSMonitor"
      },
      {
        "command": "tds-monitor.rename",
        "title": "%tds.package.rename%",
        "category": "TDSMonitor"
      },
      {
        "command": "tds-monitor.toggle",
        "title": "Monitorar",
        "category": "TDSMonitor",
        "icon": {
          "light": "resources/light/monitor.png",
          "dark": "resources/dark/monitor.png"
        }
      },
      {
        "command": "tds-monitor.show-connect-dialog",
        "title": "Abrir diálogo para parâmetro de conexão",
        "category": "TDSMonitor"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "tds-plataform",
          "title": "TDS",
          "icon": "imagens/totvs24x24.png"
        }
      ]
    },
    "views": {
      "tds-plataform": [
        {
          "id": "tds_monitor",
          "name": "Monitor"
        }
      ]
    },
    "menus": {
      "explorer/context": [],
      "view/item/context": [
        {
          "command": "tds-monitor.show-connect-dialog",
          "when": "view == tds_monitor && viewItem == monitorItem",
          "group": "AAtotvsMonitor@1"
        },
        {
          "command": "tds-monitor.toggle",
          "when": "view == tds_monitor && viewItem == monitorItem",
          "group": "AAtotvsMonitor@1"
        },
        {
          "command": "tds-monitor.create",
          "when": "view == tds_monitor && viewItem == folderItem",
          "group": "BtotvsMonitor@1"
        },
        {
          "command": "tds-monitor.rename",
          "when": "view == tds_monitor && viewItem == monitorItem",
          "group": "BtotvsMonitor@2"
        }
      ],
      "view/title": [
        {
          "command": "tds-monitor.create",
          "when": "view == tds_monitor",
          "group": "navigation"
        },
        {
          "command": "tds-monitor.open.configuration",
          "when": "view == tds_monitor",
          "group": "navigation"
        }
      ],
      "commandPalette": []
    }
  }
}
